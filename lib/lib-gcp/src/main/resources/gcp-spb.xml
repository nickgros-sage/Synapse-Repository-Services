<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd



       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config />

    <bean id="googleCloudConfig" class="org.sagebionetworks.gcp.StackGoogleCloudConfigProvider"/>
    <bean id="googleCloudStorageClient" class="org.sagebionetworks.gcp.SynapseGoogleCloudStorageClientImpl" scope="singleton" >
        <constructor-arg index="0" ref="googleCloudConfig"/>
    </bean>


</beans>